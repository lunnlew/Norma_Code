{
    "name": "lunnlew/Norma_Code",
    "description": "The Norma Framework for PHP",
    "version": "1.0.0",
    "keywords": ["Norma", "framework", "psr-3"],
    "homepage": "https://github.com/lunnlew/Norma_Code",
    "time": "2014-03-20",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "LunnLew",
        "email": "lunnlew@qq.com"
    }],
    "config": {
        "vendor-dir": "src/Norma/Vendor"
    },
    "require": {
        "php": ">=5",
        "monolog/monolog": "^1.17",
        "psr/log": "1.0.*@dev",
        "filp/whoops": "^1.1",
         "corneltek/pux": "~1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.*",
         "phpunit/php-code-coverage": "^3"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "http://packagist.phpcomposer.com"
        }
    }
}
